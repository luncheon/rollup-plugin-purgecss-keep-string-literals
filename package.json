{
  "name": "@luncheon/rollup-plugin-purgecss-keep-string-literals",
  "version": "0.0.1",
  "description": "An experimental rollup plugin that removes the rest of the CSS, leaving only the selectors used in the string literals in the JavaScript source code.",
  "license": "WTFPL",
  "repository": "luncheon/rollup-plugin-purgecss-keep-string-literals",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "tsc -p . && npm t",
    "test": "cd test && npm t"
  },
  "prettier": {
    "printWidth": 140,
    "endOfLine": "lf",
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.0",
    "acorn": "^8.0.4",
    "purgecss": "^3.0.0"
  },
  "devDependencies": {
    "@types/estree": "0.0.45",
    "@types/node": "^14.14.9",
    "typescript": "^4.1.2"
  }
}
